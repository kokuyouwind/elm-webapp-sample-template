{
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "elm": "^0.19.1-3",
    "elm-hot": "^1.1.4",
    "elm-test": "^0.19.1-revision2",
    "http-server": "^0.12.0",
    "parcel-bundler": "^1.12.4"
  },
  "scripts": {
    "start": "parcel src/index.html --out-dir dists --open",
